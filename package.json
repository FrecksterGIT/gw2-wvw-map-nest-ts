{
  "name": "gw2-ts",
  "version": "1.20.0",
  "private": true,
  "scripts": {
    "build": "run-p build:*",
    "build:fe": "webpack",
    "build:nest": "cross-env NODE_ENV=production tsc --sourceMap false",
    "coverage": "jest --coverage",
    "prebuild": "rimraf dist",
    "postbuild": "",
    "xpostbuild": "cross-env NODE_ENV=production npm-run-all -s postbuild:views postbuild:public postbuild:js postbuild:css postbuild:locales postbuild:package",
    "postbuild:css": "postcss ./src/public/stylesheets/style.css -o ./dist/public/stylesheets/style.css -u postcss-import --no-map",
    "postbuild:package": "ts-node ./scripts/prepare-dist",
    "xpostbuild:copy-emblems": "cpx ./emblems/** ./dist/emblems",
    "start": "cross-env NODE_ENV=production node dist/main",
    "start:watch": "cross-env NODE_ENV=development npm-run-all -p watch:*",
    "release": "standard-version",
    "test": "jest",
    "travis-build": "npm-run-all travis-build:*",
    "travis-build:backend": "tsc",
    "travis-build:js": "webpack",
    "travis-build:css": "postcss ./src/public/stylesheets/style.css -o ./dist/public/stylesheets/style.css -u postcss-import --no-map",
    "travis-build:imgs": "cpx ./src/public/images ./dist/public/images",
    "travis-build:views": "cpx ./src/views ./dist/views",
    "travis-build:locales": "cpx ./src/public/locales ./dist/public/locales",
    "travis-build:cert": "cpx ./src/public/.well-known ./dist/public/.well-known",
    "travis-build:package": "ts-node ./scripts/prepare-dist",
    "travis-build:copy-emblems": "cpx ./emblems ./dist/",
    "travis-build:copy-env": "cpx ./.env.example ./dist/",
    "watch:backend": "nodemon",
    "watch:frontend": "webpack",
    "watch:css": "postcss --watch ./src/public/stylesheets/style.css -o ./dist/public/stylesheets/style.css -u postcss-import --poll",
    "watch:imgs": "cpx ./src/public/images/*.{jpg,png,ico} ./dist/public/images --watch",
    "prestart:watch": "cpx ./src/public/locales/** ./dist/public/locales"
  },
  "devDependencies": {
    "@alloc/cpx": "1.6.1",
    "@babel/core": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@types/deep-diff": "1.0.2",
    "@types/i18n": "0.13.6",
    "@types/jest": "29.4.0",
    "@types/js-cookie": "3.0.3",
    "@types/node": "18.14.4",
    "@types/socket.io": "3.0.1",
    "@types/socket.io-client": "1.4.36",
    "@webcomponents/custom-elements": "1.5.1",
    "babel-loader": "9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "3.29.0",
    "cpx2": "4.2.0",
    "cross-env": "7.0.3",
    "debug": "4.3.4",
    "delegate": "3.2.0",
    "handlebars": "4.7.7",
    "i18n-for-browser": "3.0.0",
    "jest": "29.4.3",
    "js-cookie": "3.0.1",
    "nodemon": "2.0.21",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "postcss-cli": "10.1.0",
    "postcss-import": "15.1.0",
    "raw-loader": "4.0.2",
    "regenerator-runtime": "0.13.11",
    "rimraf": "4.1.3",
    "socket.io-client": "4.6.1",
    "speech-synthesis": "0.3.2",
    "standard-version": "9.5.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "tslint": "6.1.2",
    "typescript": "4.9.5",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1"
  },
  "dependencies": {
    "@nestjs/common": "9.3.9",
    "@nestjs/core": "9.3.9",
    "@nestjs/microservices": "9.3.9",
    "@nestjs/platform-express": "9.3.9",
    "@nestjs/platform-socket.io": "9.3.9",
    "@nestjs/testing": "9.3.9",
    "@nestjs/websockets": "9.3.9",
    "@svgdotjs/svg.js": "3.1.2",
    "aws-sdk": "2.1327.0",
    "compression": "1.7.4",
    "date-fns": "2.29.3",
    "deep-diff": "1.0.2",
    "dotenv-safe": "8.2.0",
    "express": "4.18.2",
    "express-handlebars": "7.0.1",
    "express-minify-html-2": "1.0.1",
    "healthcheck-middleware": "1.0.1",
    "i18n": "0.15.1",
    "jimp": "0.22.7",
    "mkdirp": "2.1.4",
    "node-fetch": "2.6.9",
    "pjson": "1.0.9",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0",
    "svgdom": "0.1.13",
    "ts-deepcopy": "0.1.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!**/node_modules/**"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
