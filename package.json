{
  "name": "gw2-ts",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "coverage": "jest --coverage",
    "postbuild": "cpx src/views/** dist/views",
    "start": "ts-node src/main",
    "start:watch": "cross-env NODE_ENV=development nodemon",
    "test": "jest"
  },
  "dependencies": {
    "@nestjs/common": "^4.6.6",
    "@nestjs/core": "^4.6.6",
    "@nestjs/microservices": "^4.6.6",
    "@nestjs/testing": "^4.6.6",
    "@nestjs/websockets": "^4.6.6",
    "@types/deep-diff": "0.0.31",
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.6",
    "@types/socket.io": "^1.4.33",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.4",
    "debug": "^3.1.0",
    "deep-diff": "^1.0.0",
    "express": "^4.16.3",
    "hbs": "^4.0.1",
    "jest": "^22.4.3",
    "node-fetch": "^2.1.2",
    "nodemon": "^1.17.3",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.10",
    "ts-jest": "^22.4.4",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
