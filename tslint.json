{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      true,
      {
        "singleline": "never",
        "multiline": "never"
      }
    ],
    "member-ordering": false,
    "one-line": false
  },
  "jsRules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      true,
      {
        "singleline": "never",
        "multiline": "never"
      }
    ],
    "one-line": false
  }
}